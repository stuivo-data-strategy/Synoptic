{
  "dataRoles": [
    {
      "displayName": "Category",
      "name": "category",
      "kind": "Grouping"
    },
    {
      "displayName": "Measure",
      "name": "measure",
      "kind": "Measure"
    }
  ],
  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "for": {
            "in": "category"
          }
        },
        "values": {
          "select": [
            {
              "bind": {
                "to": "measure"
              }
            }
          ]
        }
      }
    }
  ],
  "objects": {
    "settings": {
      "displayName": "Visual Settings",
      "properties": {
        "svgUrl": {
          "displayName": "SVG URL",
          "type": {
            "text": true
          }
        }
      }
    }
  },
  "privileges": [
    {
      "name": "WebAccess",
      "essential": true,
      "parameters": [
        "https://*"
      ]
    }
  ]
}
